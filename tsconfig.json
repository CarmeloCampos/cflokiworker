{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"noEmit": true
	},
	"exclude": ["node_modules"],
	"include": ["worker-configuration.d.ts", "src/**/*"]
}
